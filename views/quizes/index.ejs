<form method="get" action="/quizes">
	<fieldset>
	<legend>Buscar preguntas</legend>	
		<br/>
		<p><input type="text" name="buscar" placeholder="Texto a buscar" />
		<input type="submit" value="Enviar" /></p>
		<br/>
	</fieldset>
</form>
<br/>
<table>
<tr><th>Lista de preguntas</th></tr>
  <% var i; for (i=0; i < quizes.length; i++) { %>        
     <tr><td><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td></tr>
  <% } %>
</table>
<br/>
<a href="/quizes/new"><button type="button" id="btnCrear">Crear pregunta</button></a>